{
  "name": "georgringer/news-filter",
  "keywords": [
    "TYPO3",
    "extension",
    "news",
    "filter"
  ],
  "authors": [
    {
      "name": "Georg Ringer",
      "role": "Developer",
      "homepage": "https://ringer.it"
    }
  ],
  "type": "typo3-cms-extension",
  "description": "Filter news by date, categories and tags",
  "homepage": "https://ringer.it",
  "license": ["GPL-2.0-or-later"],
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5",
    "georgringer/news": "^9 || ^10"
  },
  "autoload": {
    "psr-4": {
      "GeorgRinger\\NewsFilter\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/news-filter": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "news_filter"
    }
  }
}
